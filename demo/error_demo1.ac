class Main {
    public void main() {
		/*
		what will happen if we forget to define i?
		int i;
		*/

		int year[2] = {2000, 3000};

		for (i = 0; i < 2; i = i + 1) {
			if (year[i] % 4 == 0 && year[i] % 100 != 0 || year[i] % 400 == 0) {
				print(year[i], "is leap year");
			}
			else {
				print(year[i], "is not leap year");
			}
		}
    }
}

