class Main {
    void judge(int y) {
        if (y % 4 == 0 && y % 100 != 0 || y % 400 == 0) {
            print "is leap year";
        }
        else {
            print "is not leap year";
        }
    }

    void main() {
		int i, j;
		int year[2] = {2000, 3000};

		judge(year[0]);
    }
}

