class Main {
    public void main() {
		int year;
		year = 3000;

		if (year % 4 == 0 && year % 100 != 0 || year % 400 == 0) {
			print year;
			print "is leap year\n";
		}
		else {
			print year;
			print "is not leap year\n";
		}
    }
}

